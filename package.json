{
    "name": "aelith",
    "description": "",
    "keywords": [],
    "author": "dragoncoder047",
    "main": "src/main.ts",
    "scripts": {
        "build:data": "python3 scripts/build_datapack.py",
        "build:js": "node scripts/build.mjs",
        "build:clean": "rm -rf build/ && mkdir build",
        "build": "./scripts/build_oneshot.sh",
        "dev:build:js": "node scripts/build.mjs -w",
        "dev:build:data": "pnpm nodemon -e yaml,json,png,woff,otf,ttf,txt,glsl,wav,ogg,mp3 -i build --exec \"pnpm build:data\"",
        "dev:serve": "python3 -m http.server",
        "build:minified": "pnpm build:clean && node scripts/build.mjs --minify && python3 scripts/build_datapack.py --minify",
        "dev": "pnpm run \"/^dev:.*/\"",
        "lint": "tsc && python3 scripts/lint_strings.py",
        "linecount": "for f in `git ls-files -- '[^.]**.*'`; do enca $f -L none | grep -ie \"UTF-8\\|ASCII\" -q && echo $f || true; done | xargs wc -l",
        "showip": "ifconfig | grep \"inet \" | grep -Fv 127.0.0.1 | awk '{print $2}'",
        "genframes": "python3 scripts/gen_random_frames.py",
        "test": "pnpm bun test --todo",
        "test:watch": "pnpm test --watch"
    },
    "license": "GPL-3.0-only",
    "devDependencies": {
        "@types/bun": "^1.3.9",
        "bun": "^1.3.9",
        "esbuild": "^0.25.0",
        "esbuild-plugin-glsl": "^1.4.1",
        "nodemon": "^3.1.10",
        "stacktrace-js": "^2.0.2",
        "typescript": "^5.6.2"
    },
    "dependencies": {
        "kaplay": "github:dragoncoder047/kaplay#25a76d0647adffc0d87273fd78f64c747efec0ea",
        "kaplay-lighting": "github:dragoncoder047/kaplay-lighting#943a1f2e5b0f7ad5915371087d5d0928223ef35c",
        "semver": "^7.7.4",
        "y-indexeddb": "^9.0.12",
        "y-webrtc": "^10.3.0",
        "yjs": "^13.6.29",
        "zzfx": "^1.3.0"
    },
    "type": "module",
    "pnpm": {
        "onlyBuiltDependencies": [
            "bun",
            "esbuild",
            "kaplay",
            "kaplay-lighting"
        ]
    }
}