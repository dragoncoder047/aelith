{
    "name": "aelith",
    "description": "",
    "keywords": [],
    "author": "dragoncoder047",
    "main": "src/main.ts",
    "scripts": {
        "build:data": "python3 scripts/build_datapack.py",
        "build:js": "node scripts/build.mjs",
        "build:clean": "rm -rf build/ && mkdir build",
        "build": "pnpm build:clean && pnpm build:data && pnpm build:js",
        "dev:build:js": "node scripts/build.mjs -w",
        "dev:build:data": "pnpm nodemon -e yaml,json,png,woff,otf,txt,glsl -i build --exec \"pnpm build:data\"",
        "dev:serve": "python3 -m http.server",
        "build:minified": "pnpm build:clean && node scripts/build.mjs --minify && python3 scripts/build_datapack.py --minify",
        "dev": "pnpm run \"/^dev:.*/\"",
        "lint": "tsc && python3 scripts/lint_strings.py",
        "linecount": "for f in `git ls-files -- '[^.]**.*'`; do enca $f -L none | grep -ie \"UTF-8\\|ASCII\" -q && echo $f || true; done | xargs wc -l",
        "showip": "ifconfig | grep \"inet \" | grep -Fv 127.0.0.1 | awk '{print $2}'",
        "genframes": "python3 scripts/gen_random_frames.py"
    },
    "license": "GPL-3.0-only",
    "devDependencies": {
        "esbuild": "^0.25.0",
        "esbuild-plugin-glsl": "^1.4.0",
        "nodemon": "^3.1.10",
        "stacktrace-js": "^2.0.2",
        "typescript": "^5.6.2"
    },
    "dependencies": {
        "kaplay": "github:kaplayjs/kaplay#651103404f720ecaa76ac3635409793bb80bf033",
        "kaplay-lighting": "github:tristanperrow/kaplay-lighting#2aeb008e9e214c08f2f12509209521b51ad97548",
        "zzfx": "^1.3.0"
    },
    "type": "module",
    "pnpm": {
        "ignoredBuiltDependencies": [
            "esbuild"
        ],
        "onlyBuiltDependencies": [
            "esbuild"
        ]
    }
}
